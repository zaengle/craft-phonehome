{
    "name": "zaengle/phonehome",
    "description": "Phone Home plugin for Craft CMS",
    "type": "craft-plugin",
    "version": "1.0.0",
    "keywords": [
        "craft",
        "cms",
        "craftcms",
        "craft-plugin"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Zaengle",
            "homepage": "https://zaengle.com"
        }
    ],
    "support": {
        "issues": "https://github.com/zaengle/craft-phonehome/issues",
        "source": "https://github.com/zaengle/craft-phonehome"
    },
    "require": {
        "php": ">=8.0.2",
        "craftcms/cms": "^4.0.0|^5.0.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "mockery/mockery": "^1.5"
    },
    "autoload": {
        "psr-4": {
            "zaengle\\phonehome\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "zaengle\\phonehome\\tests\\": "tests/"
        }
    },
    "extra": {
        "name": "Phone Home",
        "handle": "phonehome",
        "class": "zaengle\\phonehome\\Plugin"
    },
    "scripts": {
        "test": "phpunit --configuration tests/phpunit.xml",
        "test:unit": "phpunit --configuration tests/phpunit.xml --testsuite Unit"
    },
    "config": {
        "allow-plugins": {
            "yiisoft/yii2-composer": true,
            "craftcms/plugin-installer": true
        }
    }
}
